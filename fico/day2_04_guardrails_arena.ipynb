    "        self.competitor_keywords = [\n",
    "            \"experian\", \"transunion\", \"equifax\", \"vantagescore\",\n",
    "        ]\n",
    "    \n",
    "    def classify(self, text: str) -> str:\n",
    "        \"\"\"Classify text as on_topic, off_topic, or competitor.\"\"\"\n",
    "        text_lower = text.lower()\n",
    "        \n",
    "        # Check for on-topic keywords first\n",
    "        if any(kw in text_lower for kw in self.on_topic_keywords):\n",
    "            return \"on_topic\"\n",
    "        \n",
    "        # Check for competitor mentions\n",
    "        if any(kw in text_lower for kw in self.competitor_keywords):\n",
    "            return \"competitor\"\n",
    "        \n",
    "        # Check for off-topic keywords\n",
    "        if any(kw in text_lower for kw in self.off_topic_keywords):\n",
    "            return \"off_topic\"\n",
    "        \n",
    "        # Default: give benefit of the doubt\n",
    "        return \"on_topic\"\n",
    "    \n",
    "    def get_redirect_message(self, topic_type: str) -> str:\n",
    "        \"\"\"Get appropriate redirect message.\"\"\"\n",
    "        messages = {\n",
    "            \"off_topic\": (\n",
    "                \"I'm specialized in credit scoring and financial analytics. \"\n",
    "                \"I'd be happy to help you with:\\n\"\n",
    "                \"â€¢ How credit scores work\\n\"\n",
    "                \"â€¢ Factors that affect your credit\\n\"\n",
    "                \"â€¢ FICO products and solutions\\n\\n\"\n",
    "                \"What would you like to know about these topics?\"\n",
    "            ),\n",
    "            \"competitor\": (\n",
    "                \"I focus on FICO's solutions. While I can't speak to other \"\n",
    "                \"companies' products, I can explain how FICO approaches \"\n",
    "                \"similar challenges. Would you like to learn about FICO's capabilities?\"\n",
    "            ),\n",
    "        }\n",
    "        return messages.get(topic_type, \"\")\n",
    "\n",
    "\n",
    "# Demo\n",
    "classifier = TopicClassifier()\n",
    "\n",
    "test_questions = [\n",
    "    \"How can I improve my credit score?\",\n",
    "    \"What's a good recipe for chocolate cake?\",\n",
