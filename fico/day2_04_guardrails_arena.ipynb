    "# Interactive Demo: Prompt Injection Detection\n",
    "\n",
    "INJECTION_PATTERNS = [\n",
    "    (r\"ignore\\s+(all\\s+)?(previous|prior)\\s+instructions?\", \"instruction_override\"),\n",
    "    (r\"you\\s+are\\s+now\\s+(DAN|a\\s+different)\", \"persona_switch\"),\n",
    "    (r\"pretend\\s+(you|to)\\s+(are|be|have)\\s+no\\s+(restrictions?|rules?)\", \"restriction_bypass\"),\n",
    "    (r\"\\[system\\]|\\[admin\\]|\\[override\\]\", \"fake_system_message\"),\n",
    "    (r\"forget\\s+(everything|all|what)\\s+you\", \"memory_manipulation\"),\n",
    "    (r\"jailbreak\", \"explicit_jailbreak\"),\n",
    "]\n",
    "\n",
    "\n",
    "def detect_injection(text: str) -> list:\n",
    "    \"\"\"Detect prompt injection attempts.\"\"\"\n",
    "    detected = []\n",
    "    text_lower = text.lower()\n",
    "    \n",
    "    for pattern, attack_type in INJECTION_PATTERNS:\n",
    "        if re.search(pattern, text_lower, re.IGNORECASE):\n",
    "            detected.append(attack_type)\n",
    "    \n",
    "    return detected\n",
