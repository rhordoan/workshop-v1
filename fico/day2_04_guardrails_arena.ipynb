   "source": [
    "# Interactive Demo: PII Detection\n",
    "\n",
    "def check_for_pii(text: str) -> dict:\n",
    "    \"\"\"Check text for various types of PII.\"\"\"\n",
    "    results = {}\n",
    "    \n",
    "    patterns = {\n",
    "        \"SSN\": r\"\\b\\d{3}[-\\s]?\\d{2}[-\\s]?\\d{4}\\b\",\n",
    "        \"Credit Card\": r\"\\b(?:\\d{4}[-\\s]?){3}\\d{4}\\b\",\n",
    "        \"Phone\": r\"\\b\\d{3}[-.]?\\d{3}[-.]?\\d{4}\\b\",\n",
    "        \"Email\": r\"\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b\",\n",
    "    }\n",
    "    \n",
    "    for pii_type, pattern in patterns.items():\n",
    "        matches = re.findall(pattern, text)\n",
    "        if matches:\n",
    "            results[pii_type] = matches\n",
    "    \n",
    "    return results\n",
    "\n",
    "\n",
    "def redact_pii(text: str) -> str:\n",
    "    \"\"\"Redact PII from text.\"\"\"\n",
    "    patterns = [\n",
    "        (r\"\\b\\d{3}[-\\s]?\\d{2}[-\\s]?\\d{4}\\b\", \"[SSN REDACTED]\"),\n",
    "        (r\"\\b(?:\\d{4}[-\\s]?){3}\\d{4}\\b\", \"[CARD REDACTED]\"),\n",
    "        (r\"\\b\\d{3}[-.]?\\d{3}[-.]?\\d{4}\\b\", \"[PHONE REDACTED]\"),\n",
    "        (r\"\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b\", \"[EMAIL REDACTED]\"),\n",
    "    ]\n",
    "    \n",
    "    result = text\n",
    "    for pattern, replacement in patterns:\n",
    "        result = re.sub(pattern, replacement, result, flags=re.IGNORECASE)\n",
    "    \n",
    "    return result\n",
    "\n",
    "\n",
    "# Test examples\n",
    "test_inputs = [\n",
    "    \"My SSN is 123-45-6789 and I want to check my credit score.\",\n",
    "    \"Please charge my card 4532-1234-5678-9012 for the premium service.\",\n",
    "    \"Contact me at john.doe@example.com or 555-123-4567.\",\n",
    "    \"What factors affect my FICO score?\",  # Clean input\n",
    "]\n",
    "\n",
    "print(\"=\" * 70)\n",
    "print(\"PII DETECTION DEMO\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "for i, text in enumerate(test_inputs, 1):\n",
    "    print(f\"\\n[Example {i}]\")\n",
    "    print(f\"Input: {text}\")\n",
    "    \n",
    "    pii_found = check_for_pii(text)\n",
    "    \n",
    "    if pii_found:\n",
    "        print(f\"âš ï¸ PII DETECTED: {pii_found}\")\n",
    "        print(f\"Redacted: {redact_pii(text)}\")\n",
    "    else:\n",
    "        print(\"âœ… No PII detected - safe to process\")"
